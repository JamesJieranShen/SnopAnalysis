{
  "steps": [
    {
      "type": "tchain_provider",
      "parts": [
        {
          "tree_name": "output",
          "files": ["/nfs/disk1/users/jierans/snoplus/bismsb_roi/tagged-files/*.ntuple.root"]
        }
      ]
    },
    { "type": "describe", "short_format": true },
    { "type": "filter", "expr": "!retrigger", "comment": "No retrigger", "name": "retrigger" },
    { "type": "filter", "expr": "fitValid == true", "name": "fitValid", "comment": "fitValid" },
    { "type": "filter", "expr": "energy > 1.0 && energy < 5.0", "comment": "energy [0.6MeV, 5MeV]", "name": "Energy" },
    { "type": "filter", "expr": "nhitsCleaned > 400", "name": "nhitsCleaned" },
    {
      "type": "filter",
      "expr": "((dcApplied & 0xD82100000162C6) & dcFlagged) == (dcApplied & 0xD82100000162C6)",
      "name": "DCMask"
    },
    { "type": "filter", "expr": "posr_av < 3300", "comment": "fiducial volume R < 3.3m", "name": "FiducialVolume" },
    { "type": "filter", "expr": "!BiPo214_prompt && !BiPo214_follower", "name": "BiPo214Coincidence" },
    { "type": "filter", "expr": "!he_event_prompt && !he_event_follower", "name": "HighEnergyVeto" },
    { "type": "filter", "expr": "!hotspot_prompt && !hotspot_follower", "name": "HotspotVeto" },
    { "type": "filter", "expr": "alphaBeta212 > 0", "name": "BiPo212-AlphaBeta" },
    { "type": "report" },
    {
      "type": "snapshot",
      "tree_name": "output",
      "lazy": false,
      "file_name": "output.ntuple.root"
    }
  ]
}
